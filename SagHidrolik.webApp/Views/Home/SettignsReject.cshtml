<div class="container-claimtype">
    <div class="row">
        <div class="col-md-3">
            <div class="input-group" style="margin-top:3rem">
                <div class="input-group-prepend">
                    <span class="input-group-text " style="background-color:#e73c68;color:white" id="basic-addon1">Reject Name</span>
                </div>
                <input id="inp-settingsReject-searchRejectName" autofocus style="border-radius:0.5rem;" type="text" class="form-control" placeholder="Reject Name giriniz">
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <label for="exampleFormControlSelect1">select table row count : </label>
                <select class="form-control" id="selectRowCount-settingsReject"></select>
            </div>
        </div>
        <div class="col-md-2 form-group">
            <button id="btn-settingsReject-add" class="btn btn-success"> Add New Reject</button>
        </div>
        <div class="col-md-2 form-group">
            <button id="btn-settingsReject-reset" style="margin-top:1.8rem" class="btn btn-danger"> Reset Search</button>
        </div>
    </div>

    <div class="container-setUp-partial">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th scope="col">Reject ID</th>
                    <th scope="col">Reject Code</th>
                    <th scope="col">Reject Name</th>
                    <th scope="col">Edit</th>
                    <th scope="col">Delete</th>
                </tr>
            </thead>
            <tbody id="table-settingsReject"></tbody>
        </table>
        <div class="recordNotFound" id="recordNotFound_settingsReject">
            <h3>Hiç bir kayıt Bulunmamktadır</h3>
        </div>
        <nav aria-label="Page navigation example" class="nav-review">
            <ul class="pagination">
                <li class="page-item">
                    <a id="btn-settingsReject-previous" class="page-link" aria-label="Previous">
                        <span aria-hidden="true">&laquo;</span>
                        <span class="sr-only">Previous</span>
                    </a>
                </li>
                <li class="page-item"><a id="number-settingsReject-pageNumber" class="page-link">1</a></li>
                <li class="page-item">
                    <a id="btn-settingsReject-next" class="page-link" aria-label="Next">
                        <span aria-hidden="true">&raquo;</span>
                        <span class="sr-only">Next</span>
                    </a>
                </li>
            </ul>
        </nav>
    </div>
    <div class="tableCount"> Process toplam : <span id="settingsRejectTableCount"></span></div>
</div>

<div class="modal fade" id="settingsReject-AddModel" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">New Reject</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <label for="recipient-name" class="col-form-label">Reject Code : </label>
                        <input type="text" class="form-control" id="inp-settingsReject-recjetCode">
                    </div>
                    <div class="form-group">
                        <label for="recipient-name" class="col-form-label">Reject Name : </label>
                        <input type="text" class="form-control" id="inp-settingsReject-recjetName">
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button id="btn-settingsReject-confirmAdd" type="button" class="btn btn-primary">Add</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="settingsReject-editModel" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
