<div  class="container-stokEtiketi">
    <div class="alert defaultBackground" role="alert">
            Stok  Etiketi
    </div>
    <div>

        <div class="col-md-6">
            <div class="input-group">
                <div class="input-group-prepend">
                    <span class="input-group-text  defaultBackground" id="basic-addon1">STK</span>
                </div>
                <input id="inp-stokEtiketi-stkSearch" style="border-radius:0.5rem;" type="text" class="form-control" placeholder="ürün kodu">
            </div>
        </div>
    </div>
    <div class="col-md-12 stokEtiketiTable">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th scope="col">STK</th>
                    <th scope="col">total Stok</th>
                    <th scope="col">Tur</th>
                    <th scope="col">STA</th>
                    <th scope="col">REF</th>
                    <th scope="col">STOK REF</th>
                    <th scope="col">P_ID</th>
                    <th scope="col">fielD18</th>
                    <th scope="col">fielD19</th>
                </tr>
            </thead>
            <tbody id="table-stokEtiketi-stokEtiketiList" class=""></tbody>
        </table>
        <div id="recordNotFoundDiv_stokEtiketi" class="recordNotFoundDiv">
            <h3></h3>
        </div>
        <div class="clearFix">
        </div>
        <nav aria-label="Page navigation example" class="nav-review">
            <ul class="pagination">
                <li class="page-item">
                    <a id="btn-stokEtiketi-previous" class="page-link" href="" aria-label="Previous">
                        <span aria-hidden="true">&laquo;</span>
                        <span class="sr-only">Previous</span>
                    </a>
                </li>
                <li class="page-item"><a id="number-stokEtiketi-pageNumber" class="page-link" href="">1</a></li>
                <li class="page-item">
                    <a id="btn-stokEtiketi-next" class="page-link" href="" aria-label="Next">
                        <span aria-hidden="true">&raquo;</span>
                        <span class="sr-only">Next</span>
                    </a>
                </li>
            </ul>
        </nav>
    </div>
    <div style="clear:both"></div>
    <div style="opacity:0;" class="stokEtiketiUnderSection">
        <div class="row">
            <div class="col-md-5 col-sm-5">
                <div class="stokEtiketiCard" style="margin-top:10px;">
                    <div class="row">
                        <div class="col-md-5">
                            <label>ürün Numarası</label>
                            <p id="txt-stokEtiketi-stk"></p>
                        </div>
                        <div class="col-md-5">
                            <label>Tarih</label>
                            <input id="inp-stokEtiketi-paketletmeTarih" type="text" class="form-control" placeholder="Tarih Giriniz" aria-describedby="basic-addon1">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <label>Raf</label>
                            <p> <span id="txt-stokEtiketi-filed18"> </span>/<span id="txt-stokEtiketi-filed19"> </span> </p>

                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <label>Kutu içi Miktarı</label>
                            <p id="txt-stokEtiketi-kutuMiktari"></p>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <label> paketleme Miktari:</label>
                            <input id="inp-stokEtiketi-paketlemeMiktari" type="number" min="1" class="form-control" placeholder="paketleme miktari" aria-describedby="basic-addon1">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-7 col-sm-7" style="margin-top:10px;">
                <div class="col-md-12 stokEtiketiLastTable">
                    <button id="btn-stokEtiketi-tekEtiket" class="btn btn-success">Tek Etiket</button>
                    <button id="btn-stokEtiketi-topluEtiket" class="btn btn-success">Toplu Etiket</button>
                    <button id="btn-stokEtiketi-mamulA4" class="btn btn-primary">Mamul A4</button>
                    <button id="btn-stokEtiketi-A4" class="btn btn-primary">A4</button>
                    <button id="btn-stokEtiketi-reset" class="btn btn-primary">Reset</button>
                    <div class="zxc">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th scope="col">Stk</th>
                                    <th scope="col">Adet </th>
                                    <th scope="col">tarih</th>
                                    <th scope="col">Lot</th>
                                    <th scope="col">Lot1</th>
                                    <th scope="col">cari</th>
                                    <th scope="col">raf1</th>
                                    <th scope="col">raf2</th>
                                </tr>
                            </thead>
                            <tbody id="table-stokEtiketi-result" class=""></tbody>

                        </table>
                    </div>
                    <div class="clearFix">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

