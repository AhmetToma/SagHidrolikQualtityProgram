
<div class="container-bakimEtiketi" style="height:1500px">
    <div class="alert defaultBackground" role="alert">
        Bakım Etiketi
    </div>
    <div>
        <div class="row">
            <div class="col-md-4" style="margin-top:1.7rem;">
                <div class="input-group urunEtiketiSearchDiv">
                    <span class="input-group-text " style="background-color:#2A3F54;color:white" id="basic-addon1">Machine No</span>
                    <input autofocus type="search" id="inp-bakimEtiketi-SearchMachineNo" class="form-control " placeholder="Makine No" aria-describedby="basic-addon1">
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label for="exampleFormControlSelect1" style="color:black">select table row count : </label>
                    <select style="color:black" class="form-control" id="select-bakimEtiketi-selectRowCount"></select>
                </div>
            </div>
            <div class="col-md-2" style="margin-top:1.7rem;">
                <div class="input-group">
                    <button id="btn-bakimEtiketi-reset" class="btn btn-danger ">Reset</button>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12 bakimEtiketiTable">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th scope="col">Machine No</th>
                    <th scope="col">Machine Name </th>
                    <th scope="col">Model</th>
                    <th scope="col">Bolum </th>
                    <th scope="col">Yıl</th>
                    <th scope="col">Etiket</th>
                </tr>
            </thead>
            <tbody id="table-bakimEtiketi" class=""></tbody>
        </table>
        <div id="recordNotFoundDiv_bakimEtiketi" class="recordNotFoundDiv">
            <h3>Hiç bir kayit bulunmamaktadır</h3>
        </div>
        <div class="clearFix">
        </div>

        <nav aria-label="Page navigation example" class="nav-review">
            <ul class="pagination">
                <li class="page-item">
                    <a id="btn-bakimEtiketi-previous" class="page-link" href="" aria-label="Previous">
                        <span aria-hidden="true">&laquo;</span>
                        <span class="sr-only">Previous</span>
                    </a>
                </li>
                <li class="page-item"><a id="num-bakimEtiketi-pageNumber" class="page-link" href="">1</a></li>
                <li class="page-item">
                    <a id="btn-bakimEtiketi-next" class="page-link" href="" aria-label="Next">
                        <span aria-hidden="true">&raquo;</span>
                        <span class="sr-only">Next</span>
                    </a>
                </li>
            </ul>
        </nav>
        <div class="tableCount"> bakimEtiketi Toplam : <span id="span-bakimEtiketi-rowCount"></span></div>
    </div>
    <div style="clear:both"></div>

    <hr />
    <div style="opacity:1" class="sevkiyatKutuEtiketiUnderSection">
        <div class="row">
            <div class="col-md-12 col-sm-12">
                <div class="col-md-12 ">
                    <button id="btn-bakimEtiketi-normalEtiket" class="btn btn-primary">Normal Etiket</button>
                    <button id="btn-bakimEtiketi-kucukEtiket" class="btn btn-primary">küçük Etiket</button>
                    <button id="btn-bakimEtiketi-clearTable" class="btn btn-danger">Tablo Temizle</button>
                    <div class="zxc">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th scope="col">Mschine No </th>
                                    <th scope="col">Machine Name </th>
                                    <th scope="col">Model</th>
                                    <th scope="col">Bolum</th>
                                    <th scope="col">Yıl</th>
                                    <th scope="col">Delete</th>
                                </tr>
                            </thead>
                            <tbody id="table-bakimEtiketi-result" class=""></tbody>
                        </table>
                    </div>
                    <div class="clearFix">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



<div class="bakimEtiketiPrintModel">

</div>


