
    <div class="container-claimtype">
        <div class="row">

            <div class="col-md-3">
                <div class="input-group" style="margin-top:2.8rem">
                    <div class="input-group-prepend">
                        <span class="input-group-text " style="background-color:#071f0c;color:white" id="basic-addon1">STK</span>
                    </div>
                    <input id="inp-supplierPerfReport-searchStk" style="border-radius:0.5rem;" type="text" class="form-control" placeholder="ürün kodu">
                </div>
            </div>
            <div class="col-md-2">
                <div class="form-group">
                    <label for="exampleFormControlSelect1">row count : </label>
                    <select class="form-control" id="selectRowCount-supplierPerfReport">
                        <option value="6">6</option>
                        <option value="10">10</option>
                        <option value="100">100</option>
                        <option value="500">500</option>
                    </select>
                </div>
            </div>
            <div class="col-md-2">
                <div class="form-group">
                    <label for="exampleFormControlSelect1">Year : </label>
                    <select class="form-control" id="select-supplierPerfReport-year">
                        <option value="">...</option>
                        @for (int i = 2017; i <= DateTime.Now.Year; i++)
                        {
                            <option value="@i ">@i</option>
                        }
                    </select>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label for="exampleFormControlSelect1">Month :</label>
                    <select class="form-control" id="select-supplierPerfReport-month">
                        <option value="" selected>...</option>
                        <option value="1">Ocak</option>
                        <option value="2">şubat</option>
                        <option value="3">mart</option>
                        <option value="4">nisan</option>
                        <option value="5">mayıs</option>
                        <option value="6">haziran</option>
                        <option value="7">temmus</option>
                        <option value="8">Ağustos</option>
                        <option value="9">Eylül</option>
                        <option value="10">Ekim</option>
                        <option value="11">Kasım</option>
                        <option value="12">Aralık</option>
                    </select>
                </div>
            </div>

            <div class="col-md-2">
                <div class="form-group" >
                    <button id="btn-supplierPerfReport-exportToExcel" class="btn btn-sm btn-success">export to excel</button>
                    <button id="btn-supplierPerfReport-resetSearch" style="background-color:#b13e5b;color:white" class="btn btn-sm btn-success ">Reset Search</button>
                </div>
            </div>
        </div>

        <div class="container-setUp-partial">
            <table id="table-supplierPerfReport-xls" class="table table-bordered">
                <thead>
                    <tr>
                        <th scope="col">STK</th>
                        <th scope="col">STA</th>
                        <th scope="col">SiperVranko</th>
                        <th scope="col">OrderQty</th>
                        <th scope="col">TestTarihi</th>
                        <th scope="col">StokAlt Tarhi</th>
                        <th scope="col">Total Invoice</th>
                        <th scope="col">Turac</th>
                        <th scope="col">Tur</th>
                    </tr>
                </thead>
                <tbody id="table-supplierPerfReport">
                </tbody>
            </table>
            <div class="recordNotFound" id="recordNotFound_supplierPerfReport">
                <h3>Hiç bir kayıt Bulunmamktadır</h3>
            </div>
            <nav aria-label="Page navigation example" class="nav-review">
                <ul class="pagination">
                    <li class="page-item">
                        <a id="btn-supplierPerfReport-previous" class="page-link" aria-label="Previous">
                            <span aria-hidden="true">&laquo;</span>
                            <span class="sr-only">Previous</span>
                        </a>
                    </li>
                    <li class="page-item"><a id="number-supplierPerfReport-pageNumber" class="page-link">1</a></li>
                    <li class="page-item">
                        <a id="btn-supplierPerfReport-next" class="page-link" aria-label="Next">
                            <span aria-hidden="true">&raquo;</span>
                            <span class="sr-only">Next</span>
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
        <div class="tableCount"> production Report : <span id="supplierPerfReportTableCount"></span></div>
    </div>