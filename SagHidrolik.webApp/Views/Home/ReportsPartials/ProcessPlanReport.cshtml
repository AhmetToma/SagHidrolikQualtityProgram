                        <span class="input-group-text " style="background-color:#071f0c;color:white" id="basic-addon1">STK</span>
                    </div>
                    <input id="inp-processPlanReport-searchStk" style="border-radius:0.5rem;" type="text" class="form-control" placeholder="ürün kodu">
                </div>
            </div>

            <div class="col-md-2">
                <div class="form-group">
                    <label for="exampleFormControlSelect1">Select Group : </label>
                    <select style="width:100%" class="form-control" id="selectGroup-processPlanReport">
