                        <th scope="col">Wo New Date </th>
                        <th scope="col">Balance</th>
                        <th scope="col">Delete</th>
                    </tr>
                </thead>
                <tbody id="table-processPlanReport">
                </tbody>
            </table>
            <div class="recordNotFound" id="recordNotFound_processPlanReport">
                <h3>Hiç bir kayıt Bulunmamktadır</h3>
            </div>
            <nav aria-label="Page navigation example" class="nav-review">
                <ul class="pagination">
                    <li class="page-item">
                        <a id="btn-processPlanReport-previous" class="page-link" aria-label="Previous">
                            <span aria-hidden="true">&laquo;</span>
                            <span class="sr-only">Previous</span>
                        </a>
                    </li>
                    <li class="page-item"><a id="number-processPlanReport-pageNumber" class="page-link">1</a></li>
                    <li class="page-item">
                        <a id="btn-processPlanReport-next" class="page-link" aria-label="Next">
                            <span aria-hidden="true">&raquo;</span>
                            <span class="sr-only">Next</span>
                        </a>
                    </li>
                </ul>
            </nav>
