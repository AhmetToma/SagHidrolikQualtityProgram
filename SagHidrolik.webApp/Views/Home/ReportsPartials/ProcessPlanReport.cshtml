                        <option value="01_Kesim">01_Kesim</option>
                        <option value="02_Büküm">02_Büküm</option>
                        <option value="03_Havşa">03_Havşa</option>
                        <option value="04_Kaynak">04_Kaynak</option>
                        <option value="041_KesDel">041_KesDel</option>
                        <option value="042_Hazirlik">042_Hazirlik</option>
                        <option value="05_Hortum">05_Hortum</option>
                        <option value="06_Paketleme">06_Paketleme</option>
                        <option value="06_PaketlemeDiğer">06_PaketlemeDiğer</option>
                        <option value="06_test">06_test</option>
                        <option value="07_Mercedes">07_Mercedes</option>
                        <option value="09_Kaplama">09_Kaplama</option>
                        <option value="08_UçŞekil">08_UçŞekil</option>
                        <option value="99_Proto">99_Proto</option>
                    </select>
                </div>
            </div>
            <div class="col-md-2">
                <div class="form-group">
                    <label for="exampleFormControlSelect1">row count : </label>
                    <select style="width:100%" class="form-control" id="selectRowCount-processPlanReport">
                    </select>
                </div>
            </div>
            <div class="col-md-2">
                <div class="form-group">
                    <label for="exampleFormControlSelect1">Year : </label>
                    <select style="width:100%" class="form-control" id="select-processPlanReport-year">
                        <option value="">...</option>
                        @for (int i = 2017; i <= DateTime.Now.Year; i++)
                        {
                            <option value="@i ">@i</option>
                        }
                    </select>
                </div>
            </div>
            <div class="col-md-2">
                <div class="form-group">
                    <label for="exampleFormControlSelect1">Month :</label>
                    <select style="width:100%" class="form-control" id="select-processPlanReport-month">
                        <option value="" selected>...</option>
                        <option value="1">Ocak</option>
                        <option value="2">şubat</option>
                        <option value="3">mart</option>
                        <option value="4">nisan</option>
                        <option value="5">mayıs</option>
                        <option value="6">haziran</option>
                        <option value="7">temmus</option>
                        <option value="8">Ağustos</option>
                        <option value="9">Eylül</option>
                        <option value="10">Ekim</option>
                        <option value="11">Kasım</option>
                        <option value="12">Aralık</option>
                    </select>
                </div>
            </div>

            <div class="col-md-1">
                <div class="form-group" style="margin-top:2.8rem">
                    <button id="btn-processPlanReport-resetSearch" class="btn btn-primary">Reset Search</button>
                </div>
            </div>
        </div>

        <div class="container-setUp-partial">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th scope="col">STK</th>
                        <th scope="col">Date</th>
                        <th scope="col">Group</th>
                        <th scope="col">Process Adi</th>
                        <th scope="col">WOLot</th>
