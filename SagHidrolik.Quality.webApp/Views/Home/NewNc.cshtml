            <div>
                <label class="demo-form-label">NC Resposible : </label>

                <select id="select-newNc-resposible" class="limitedNumbSelect2 form-control firstSelect" multiple="true"></select>
            </div>
            <div>
                <label class="demo-form-label">NC Id Def :</label>
                <input type="text" id="inp-newNC-def" class="form-control" name="fullname" required />
            </div>
            <div>
                <label class="demo-form-label">Non conformity :</label>
                <textarea rows="2" id="inp-newNC-conformity" class="form-control"></textarea>
            </div>

            <div>
                <label class="demo-form-label">Nc Description :</label>
                <textarea rows="2" id="inp-newNC-description" class="form-control"></textarea>
            </div>
            <div>
                <label class="demo-form-label">QTY : </label>
                <input min="0" type="number" id="inp-newNC-qty" class="form-control" name="fullname" required />
            </div>


            <div>
                <label class="demo-form-label">NC Open Date</label>
                <input type="text" id="inp-newNc-openDate" class="form-control" name="fullname" required />

            </div>
            <div>
                <label class="demo-form-label">NC Target Date : </label>
                <input type="text" id="inp-newNc-tergetDate" class="form-control" name="fullname" required />
            </div>

            <div>
                <label class="demo-form-label">NC Close Date : </label>
                <input type="text" id="inp-newNc-closeDate" class="form-control" name="fullname" required />
            </div>

        </div>
    </div>

    <input id="btn-newNc-submit" class="btn btn-dark" type="submit" value="Kaydet" />
    <input  id="btn-newNc-reset" class="btn btn-danger" type="submit" value="Reset" />
