<form class="demo-form">
    <div class="row">
        <div class="col-md-6">
            <div>
                <label class="demo-form-label">NC Type :</label>
                <select required id="select-newNc-ncType" class="limitedNumbSelect2 form-control firstSelect" multiple="true"></select>
            </div>
            <div>
                <label  class="demo-form-label">Activity Type :</label>
                <br />
                <div  class="pretty p-icon p-curve p-jelly mt-2 ">
                    <input value="Corrective" type="radio" name="radio66">
                    <div class="state p-success">
                        <i style="margin-top: 5%;"  class="icon mdi mdi-check"></i>
                        <label style="font-size: 1rem"> Corrective</label>
                    </div>
                </div>
                <br />

                <div class="pretty p-icon p-curve p-jelly mt-2 ">
                    <input value="Preventative" type="radio" name="radio66">
                    <div class="state p-success">
                        <i style="margin-top: 5%;"  class="icon mdi mdi-check"></i>
                        <label style="font-size: 1rem"> Preventative</label>
                    </div>
                </div>


                <br />

                <div class="pretty p-icon p-curve p-jelly mt-2 mb-4">
                    <input value="Repetitive" type="radio" name="radio66">
                    <div class="state p-success">
                        <i style="margin-top: 5%;" class="icon mdi mdi-check"></i>
                        <label style="font-size: 1rem"> Repetitive</label>
                    </div>
                </div>



            </div>

            <div>
                <label class="demo-form-label">NC Customer Supplier :</label>
